{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "CORS_ORIGINS": "https://frenchnoveltool.com,https://www.frenchnoveltool.com"
  }
}
